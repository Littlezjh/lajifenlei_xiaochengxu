/* pages/camera/camera.wxss */
page {
  height: 100%;
}

.container {
  padding: 0rpx;
  height: 100%;
}

.container .camera-wrapper {
  padding: 0rpx;
  height: 90%;
}

.container .camera {
  width: 100%;
  height: 100%;
}

.container .camera-control {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 100rpx;
  color: white;
  background-color: black;
  height: 10%
}
.click{
  padding: 0;
  width: 50%;
  height: 80%;
}
.content{
  font-weight: normal;
  font-size: 90%;
}
