page {
  height: 90%;
}

.container {
  height: 100%;
}

.openCamera {
  height: 47.5%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  border: 1rpx solid #ddd;
  border-style: groove;
  padding: 10rpx 0 10rpx;
  margin: 10rpx 0 50rpx;
  box-shadow: 0 0 20rpx #ddd;
  border-radius: 20rpx;
  opacity: 1;
}

.searchGarbage {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 47.5%;
  align-items: center;
  border: 1rpx solid #ddd;
  border-style: groove;
  margin: 10rpx 0;
  padding: 10rpx 0;
  box-shadow: 0 0 20rpx #ddd;
  border-radius: 20rpx;
  opacity: 1;
}

.image1 {
  width: 100%;
  height: 100%;
  padding: 10rpx;
}

.image2 {
  width: 80%;
  height: 80%;
  padding: 20rpx;
}

.click {
  padding: 10rpx 0;
  font-weight: bold;
}
